<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <commands>
      <command name="cognitive:viewanalaysis" type="Sitecore.SharedSource.CognitiveServices.ImageSearch.Commands.ViewAnalysis,Sitecore.SharedSource.CognitiveServices.ImageSearch"/>
      <command name="cognitive:reanalyze" type="Sitecore.SharedSource.CognitiveServices.ImageSearch.Commands.Reanalyze,Sitecore.SharedSource.CognitiveServices.ImageSearch"/>
      <command name="cognitive:reanalyzeall" type="Sitecore.SharedSource.CognitiveServices.ImageSearch.Commands.ReanalyzeAll,Sitecore.SharedSource.CognitiveServices.ImageSearch"/>
      <command name="cognitive:setalttags" type="Sitecore.SharedSource.CognitiveServices.ImageSearch.Commands.SetAltTags,Sitecore.SharedSource.CognitiveServices.ImageSearch"/>
      <command name="cognitive:setalttagsall" type="Sitecore.SharedSource.CognitiveServices.ImageSearch.Commands.SetAltTagsAll,Sitecore.SharedSource.CognitiveServices.ImageSearch"/>
    </commands>
    <services>
      <configurator type="Sitecore.SharedSource.CognitiveServices.ImageSearch.App_Start.IocConfig, Sitecore.SharedSource.CognitiveServices.ImageSearch" />
    </services>
    <settings>
      <setting name="CognitiveService.ImageSearch.Language" value="en" />
      <setting name="CognitiveService.ImageSearch.TextualFieldTypes" value="Rich Text|Single-Line Text|Multi-Line Text|html|text|memo" />
      <setting name="CognitiveService.ImageSearch.IndexNameFormat" value="cognitive_imagesearch_{0}_index" />
    </settings>
  </sitecore>
</configuration>